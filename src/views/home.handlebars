<div>
    <h1>Products</h1>
    <ul>
        {{#each totalProducts}}
            <li>
                <p>Id: {{this._id}}</p>
                <p>Name: {{this.title}}</p>
                <p>Description: {{this.description}}</p>
                <p>Available stock: {{this.stock}}</p>
            </li>   
        {{/each}}

    </ul>

</div>

<script src="/socket.io/socket.io.js"></script>
<script src="/static/js/index.js"></script>